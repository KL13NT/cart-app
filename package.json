		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.7.5",
		"@typescript-eslint/parser": "^6.7.5",
		"@vitejs/plugin-react": "^4.0.3",
		"@vitest/coverage-istanbul": "^0.34.6",
